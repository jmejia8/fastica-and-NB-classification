head(promedios)###Indica la clase de objetos
lm.Femenino<-lm(egresado ~ bachillerato, data=promedios, subset=sexos=="Femenino")
lm.Femenino<-lm(egresado ~ bachillerato, data=promedios, subset=sexos=="Femenino")
lm.Masculino<-lm(egresado ~ bachillerato, data=promedios, subset=sexos=="Masculino")
plot(egresado ~ bachillerato, data=promedios, type="p")
abline(lm.Femenino)
abline(lm.Masculino, lty=2)
legend("bottomright", title="Sexo", legend=c("Femenino", "Masculino"), lty=c(1,2))
summary(lm.Femenino)
summary(lm.Masculino)
lm.promedios<-lm(egresado ~ bachillerato + sexo, data=promedios)
summary(lm.promedios)
lm.promediosplus<-lm(egresado ~ bachillerato + sexo +bachillerato*sexo, data=promedios)
summary(lm.promediosplus)
confint(lm.promediosplus)##Intervalos de confianza
par(mfrow=c(2,2))
plot(lm.promediosplus)
